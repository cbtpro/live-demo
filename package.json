{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write . --plugin-search-dir=./node_modules/ --plugin=@trivago/prettier-plugin-sort-imports",
    "check:format": "prettier --check . --plugin-search-dir=./node_modules/ --plugin=@trivago/prettier-plugin-sort-imports",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^12.4.0",
    "d3": "^7.9.0",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.17.13",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "husky": "^8.0.3",
    "less": "^4.2.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.5",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}

{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write . --plugin-search-dir=./node_modules/ --plugin=@trivago/prettier-plugin-sort-imports",
    "check:format": "prettier --check . --plugin-search-dir=./node_modules/ --plugin=@trivago/prettier-plugin-sort-imports",
    "prepare": "husky install"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "pinia": "^2.1.4",
    "vue": "^3.2.25",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.4.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "husky": "8.0.3",
    "less": "^4.1.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}

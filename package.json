{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write . --plugin-search-dir=./node_modules/ --plugin=@trivago/prettier-plugin-sort-imports",
    "check:format": "prettier --check . --plugin-search-dir=./node_modules/ --plugin=@trivago/prettier-plugin-sort-imports",
    "prepare": "husky install",
    "vite-inspect": "npx serve .vite-inspect"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "pinia": "^2.1.4",
    "vant": "^4.9.0",
    "vue": "^3.2.25",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/d3": "^7.4.3",
    "@types/d3-array": "^3.2.1",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/node": "^20.4.5",
    "@vant/auto-import-resolver": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.4.27",
    "autoprefixer": "^10.4.19",
    "husky": "8.0.3",
    "less": "^4.1.3",
    "lint-staged": "13.2.3",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prettier": "3.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^4.4.7",
    "vite-plugin-inspect": "^0.8.4",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
